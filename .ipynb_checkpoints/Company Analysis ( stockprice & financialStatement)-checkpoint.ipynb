   "source": [
    "# 各企業の5年間の平均利益率を見てみる(愛知銀行はNan)\n",
    "incomeAVG_query = ''' SELECT company_id, company.name, AVG(operating_income / sales) \n",
    "                      FROM financialStatement\n",
    "                      JOIN company\n",
    "                      ON financialStatement.company_id = company.id\n",
    "                      GROUP BY company_id;'''\n",
    "incomeAVG = sql_to_df(incomeAVG_query)\n",
    "incomeAVG"
   ]
  },
